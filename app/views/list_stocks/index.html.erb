<div class="panel panel-dafault">
	<div class="panel-heading">Activos del Sistema</div>
	<div align="right"><td><%= link_to 'Nueva', new_list_stock_path, class: "btn btn-default" %></td></div>
	<table class="table">
	    <thead>
	      <tr>
	        <th>Nombre</th>
	        <th>Símbolo</th>
	        <th>Mercado</th>
	        <th></th>
	        <th></th>
	      </tr>
	    </thead>

	    <tbody>
	      <% @saved_stocks.each do |ss| %>
	        <tr>
	          <td><%= ss.complete_name %></td>
	          <td><%= ss.name %></td>
	          <td><%= ss.market %></td>
	          <td><%= link_to "Editar", edit_list_stock_path(ss.id) %></td>
	          <td><%= link_to "Eliminar", list_stock_path(ss), method: :delete, data: { confirm: "Estás seguro?" } %></td>
	        </tr>
	      <% end %>
	    </tbody>
	</table>
</div>
